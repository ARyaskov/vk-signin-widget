{
  "name": "@riaskov/vk-signin-widget",
  "description": "VK ID Signin Widget for React",
  "version": "1.0.8",
  "license": "MIT",
  "keywords": [
    "react",
    "vk",
    "vkid",
    "vk id",
    "widget",
    "signin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ARyaskov/vk-signin-widget.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "author": {
    "name": "Andrei Riaskov",
    "email": "code@riaskov.com"
  },
  "targets": {
    "default": {
      "engines": {
        "browsers": ">= 50%"
      }
    },
    "main": {
      "optimize": true
    }
  },
  "scripts": {
    "build:webpage": "npx parcel build src/test-page/index.html",
    "build": "npx parcel build --no-source-maps src/vk-signin-button.tsx && yarn run copy-types",
    "start:dev": "npx parcel src/test-page/index.html",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "copy-types": "copyfiles -f src/*.d.ts dist/"
  },
  "dependencies": {
    "@vkid/sdk": "^1.1.0",
    "classnames": "^2.5.1",
    "prop-types": "^15.6.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@parcel/config-default": "2.12.0",
    "@parcel/packager-ts": "2.12.0",
    "@parcel/reporter-dev-server": "2.12.0",
    "@parcel/transformer-sass": "2.12.0",
    "@parcel/transformer-typescript-tsc": "2.12.0",
    "@parcel/transformer-typescript-types": "2.12.0",
    "@types/node": "20.12.12",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.11",
    "copyfiles": "^2.4.1",
    "parcel": "2.12.0",
    "prettier": "^4.0.0-alpha.8",
    "process": "^0.11.10",
    "typescript": "5.4.5"
  }
}
